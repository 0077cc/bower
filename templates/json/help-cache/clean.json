{
    "command": "cache list",
    "description": "Cleans cached packages",
    "usage": [
        "cache clean",
        "cache clean <name> [<name> ...]",
        "cache clean <name>#<version> [<name>#<version> ..]"
    ],
    "options": [
        {
            "shorthand":   "-c",
            "flag":        "--completion",
            "description": "Clear the completion cache"
        },
        {
            "shorthand":   "-h",
            "flag":        "--help",
            "description": "Show this help message"
        }
    ]
}